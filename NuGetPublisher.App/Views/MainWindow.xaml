<Window x:Class="NuGetPublisher.App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:NuGetPublisher.App.Views"
        xmlns:app="clr-namespace:NuGetPublisher.App"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel>
            <views:LoginView DockPanel.Dock="Top"></views:LoginView>
            <views:PackageListView DockPanel.Dock="Left"></views:PackageListView>
        </DockPanel>
    </Grid>
</Window>

